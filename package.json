{
  "name": "manginx",
  "version": "0.0.1",
  "description": "An cli manager for nginx.",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint src",
    "coverage": "NODE_ENV=test nyc --all mocha src/**/*.spec.js src/*.spec.js",
    "test": "NODE_ENV=test mocha src/**/*.spec.js src/*.spec.js"
  },
  "keywords": [
    "node",
    "nginx",
    "template",
    "configuration",
    "install",
    "uninstall"
  ],
  "private": true,
  "author": "christophe genin",
  "license": "ISC",
  "preferGlobal": true,
  "bin": {
    "manginx": "./index.js"
  },
  "nyc": {
    "include": [
      "src/**/**.js",
      "src/*.js"
    ],
    "exclude": [
      "src/**/**.spec.js"
    ],
    "reporter": [
      "html",
      "text-summary"
    ]
  },
  "dependencies": {
    "colors": "^1.2.1",
    "commander": "^2.17.1",
    "fast-safe-stringify": "^2.0.6",
    "fkill": "^5.3.0",
    "fs-extra": "^7.0.0",
    "handlebars": "^4.0.11",
    "inquirer": "^6.1.0",
    "lokijs": "^1.5.5",
    "mock-require": "^3.0.2",
    "path-is-absolute": "^1.0.1",
    "rewiremock": "^3.7.7",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2",
    "temp-dir": "^1.0.0",
    "triple-beam": "^1.3.0",
    "uuid": "^3.3.2",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "co-mocha": "^1.2.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "sinon": "^6.1.5"
  }
}
